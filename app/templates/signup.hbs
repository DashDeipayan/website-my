{{#if this.what}}
  {{#if (eq this.what "get-started")}}
    <SignUp::GetStarted @changeRouteParams={{this.changeRouteParams}} @what={{this.what}} />
  {{else if (eq this.what "thank-you")}}
    <SignUp::GetStarted @changeRouteParams={{this.changeRouteParams}} @what={{this.what}}/>
  {{else}}
    <SignUp::FormInput
      @changeRouteParams={{this.changeRouteParams}}
      @register={{this.registerUser}}
      @firstName={{this.firstName}}
      @lastName={{this.lastName}}
      @userName={{this.userName}}
      @what={{this.what}}
      @isSubmitClicked={{this.isSubmitClicked}}
    />
  {{/if}}
{{/if}}
<div class="login-register__error">
  <p> {{this.errorMessage}} </p>
</div>